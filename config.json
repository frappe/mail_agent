{
    "haraka": {
        "agent_type": "${HARAKA_AGENT_TYPE}",
        "me": "frappemail.com",
        "tls_key_path": null,
        "tls_cert_path": null,
        "received_header": "Frappe Mail Agent",
        "retries": 3,
        "frequency": 0,
        "username": "${HARAKA_USERNAME}",
        "password": "${HARAKA_PASSWORD}",
        "port": "${HARAKA_PORT}",
        "plugins": {
            "Inbound": [
                "tls",
                "headers",
                "aliases",
                "mailauth",
                "inbound_hooks"
            ],
            "Outbound": [
                "tls",
                "headers",
                "haraka-plugin-auth-enc-file",
                "outbound_hooks"
            ]
        }
    },
    "rabbitmq": {
        "host": "${RABBITMQ_HOST}",
        "port": "${RABBITMQ_PORT}",
        "username": "${RABBITMQ_USERNAME}",
        "password": "${RABBITMQ_PASSWORD}"
    },
    "queues": {
        "mail::outgoing_mail": {
            "durable": true
        },
        "mail::outgoing_mails": {
            "durable": true
        }
    },
    "consumers": {
        "mail::outgoing_mail": {
            "callback": "print_message",
            "auto_ack": false,
            "workers": 3
        },
        "mail::outgoing_mails": {
            "callback": "print_message",
            "auto_ack": false,
            "workers": 5
        }
    }
}